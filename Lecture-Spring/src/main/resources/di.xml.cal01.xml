<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans 
	   					   http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean class="di.xml.cal01.Calculator" id="cal" />
	<bean class="di.xml.cal01.MyCalculator" id="myCal">
		<property name="firstNum" value="20"></property>
		<property name="secondNum" value="10"></property>
		<property name="calculator" ref="cal"></property>
	</bean>
		
</beans>